<h3>Comp2: @NickName   -@Age</h3>
<p>Sex: @Sex</p>
@code {

    [CascadingParameter(Name = "NickName")]
    public string? NickName { get; set; }

    [CascadingParameter(Name = "Age")]
    public int Age { get; set; }

    [CascadingParameter(Name = "Sex")]
    public string? Sex { get; set; }

}
